{
  "version": 2,
  "builds": [
    {
      "src": "src/app.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["node_modules/.prisma/**", "prisma/schema.prisma"]
      }
    }
  ],
  "build": {
    "env": {
      "PRISMA_GENERATE": "true"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/app.js"
    }
  ]
}